# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=myuser
DB_PASSWORD=mypassword
DB_NAME=mydb
DB_SSLMODE=disable

# JWT Configuration
# Access token duration (examples: 15m, 1h, 30s)
JWT_ACCESS_TOKEN_DURATION=15m
# Refresh token duration (examples: 7d, 168h, 604800s)
JWT_REFRESH_TOKEN_DURATION=168h
# RSA key size in bits (2048, 3072, 4096)
JWT_RSA_KEY_SIZE=2048